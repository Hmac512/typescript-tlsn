{
  "name": "typescript-npm-package-template",
  "version": "0.0.0-development",
  "description": "A template for creating npm packages using TypeScript and VSCode",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf ./lib/",
    "lint": "eslint ./src/ --fix",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/ryansonshine/typescript-npm-package-template/issues"
  },
  "homepage": "https://github.com/ryansonshine/typescript-npm-package-template#readme",
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^12.20.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.2.0",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@mysten/bcs": "^0.9.1",
    "@napi-rs/blake-hash": "^1.3.4",
    "@noble/curves": "^1.3.0",
    "@stablelib/chacha": "^1.0.1",
    "bn.js": "^5.2.1",
    "chacha": "^2.1.0",
    "lodash": "^4.17.21",
    "tree-range-set": "^1.0.4"
  }
}
